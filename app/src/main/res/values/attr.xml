<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DuoDrawerLayout">
        <attr name="menu" format="reference"/>
        <attr name="content" format="reference"/>
        <attr name="contentScaleClosed" format="float"/>
        <attr name="clickToCloseScale" format="float"/>
        <attr name="contentScaleOpen" format="float"/>
        <attr name="menuScaleClosed" format="float"/>
        <attr name="menuScaleOpen" format="float"/>
        <attr name="menuAlphaClosed" format="float"/>
        <attr name="menuAlphaOpen" format="float"/>
        <attr name="marginFactor" format="float"/>
    </declare-styleable>
    <declare-styleable name="DuoMenuView">
        <attr name="header" format="reference"/>
        <attr name="footer" format="reference"/>
        <attr name="background" format="reference"/>
    </declare-styleable>
    <declare-styleable name="ds">
        <attr name="backgroundColor" format="color"/>
        <attr name="cardBackground" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="tintColor" format="color"/>
        <attr name="buttonColor" format="color"/>
        <attr name="backgroundTint" format="color"/>
        <attr name="backgroundTintForFabPresent" format="color"/>
        <attr name="backgroundTintForFabAbsent" format="color"/>
        <attr name="backgroundTintForFab" format="color"/>
        <attr name="backgroundForHeader" format="color"/>
        <attr name="navigationItemTintColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="MaterialDialog">
        <attr name="material_dialog_background" format="color|reference" />

        <attr name="material_dialog_title_text_color" format="color|reference" />
        <attr name="material_dialog_message_text_color" format="color|reference" />

        <attr name="material_dialog_positive_button_text_color" format="color|reference" />
        <attr name="material_dialog_positive_button_color" format="color|reference" />

        <attr name="material_dialog_negative_button_text_color" format="color|reference" />
    </declare-styleable>
</resources>